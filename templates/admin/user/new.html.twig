{% extends 'admin/base.html.twig' %}
{% import "macros/breadcrumb.html.twig" as breadcrumb %}
{% import "macros/link.html.twig" as link %}
{% block title %}
	{{'New User'|trans}}
{% endblock %}
{% block breadcrumb %}
	{{ breadcrumb.breadcrumb('Manage user'|trans,[
        {
            path:path('app_admin_user_index'),
            name:'Manage orders'|trans
        }
    ])}}
{% endblock %}
{% block body %}
	<div class="card card-outline card-primary">
		<div class="card-header">
			<h3>{{'Create new User'|trans}}</h3>
		</div>
		<div class="card-body">
			{{ include('admin/user/_form.html.twig') }}
		</div>
	</div>
	{{ link.back(path('app_admin_user_index')) }}

{% endblock %}
